{
    "name": "rstar-ui-dev",
    "version": "0.0.1-alpha.2",
    "main": "dist/index.js",
    "type": "module",
    "private": true,
    "packageManager": "pnpm@8.8.0",
    "scripts": {
        "play": "pnpm -C play dev",
        "docs:dev": "pnpm -C docs dev",
        "docs:build": "pnpm -C docs build",
        "docs:serve": "pnpm -C docs serve",
        "build": "node scripts/build.js",
        "build:lib": "node scripts/build-lib.js",
        "build:preset": "pnpm --filter @rstar-ui/preset run build",
        "publish:npm": "pnpm publish --no-git-checks --access public",
        "lint": "eslint ./packages/**/src --ext .js,.ts,.tsx,.vue,.mjs,.cjs",
        "lint:fix": "eslint ./packages/**/src --ext .js,.ts,.tsx,.vue,.mjs,.cjs --fix",
        "clean": "node scripts/clean.js",
        "clean:deps": "node scripts/clean-deps.js",
        "prettier:check": "prettier --check .",
        "prettier:fix": "prettier --write ."
    },
    "author": "jackma9604",
    "license": "MIT",
    "description": "",
    "dependencies": {
        "@vitejs/plugin-vue": "^4.2.3",
        "unocss": "^0.56.5",
        "vite": "^4.4.5",
        "vue": "^3.3.4"
    },
    "devDependencies": {
        "@types/node": "^20.7.1",
        "@typescript-eslint/eslint-plugin": "^6.8.0",
        "@typescript-eslint/parser": "^6.8.0",
        "@unocss/preset-mini": "^0.56.5",
        "@vitejs/plugin-vue-jsx": "^3.0.2",
        "eslint": "^8.51.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-vue": "^9.17.0",
        "execa": "^8.0.1",
        "fs-extra": "^11.1.1",
        "husky": "^8.0.3",
        "jsdom": "^22.1.0",
        "lint-staged": "^15.0.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.0.3",
        "rimraf": "^5.0.5",
        "rslog": "^1.1.0",
        "tslib": "^2.6.2",
        "tsup": "^7.2.0",
        "tsx": "^3.13.0",
        "typescript": "^5.2.2",
        "unplugin-vue-macros": "^2.6.0",
        "vite-plugin-dts": "^3.6.0",
        "vitest": "^0.34.5",
        "vue-eslint-parser": "^9.3.2",
        "vue-tsc": "^1.8.5"
    },
    "lint-staged": {
        "*.{js,vue,jsx,tsx}": [
            "prettier --write",
            "eslint --cache --fix",
            "git add"
        ]
    }
}